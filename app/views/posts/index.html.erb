<h1>Taze tahminler</h1>

<table>
  <tr>
    <th>Kod</th>
    <th>Ev sahibi</th>
    <th>Misafir</th>
    <th>Tahmin</th>
    <th>Oran</th>
    <th>Uzman</th>
    
  </tr>
	
<% @posts.each do |post| %>
  <tr>
    <td><%= post.kod %></td>
    <td><%= post.ev_sahibi %></td>
    <td><%= post.misafir %></td>
    <td><%= post.tahmin %></td>
    <td><%= post.oran %></td>
    <td><%= post.user.try(:username) %></td>  
  </tr>
  
<% end %>
</table>

